# GPT Researcher Full Stack Environment Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# GPT Researcher Configuration
RESEARCH_TYPE=research_report
MAX_ITERATIONS=3

# Legal Enhancement Configuration
USE_ENHANCED_PROCESSING=true
USE_SEMANTIC_CHUNKING=true
EMBED_MODEL_NAME=BAAI/bge-large-en-v1.5
SIMILARITY_THRESHOLD=0.75

# OWUI Configuration
WEBUI_SECRET_KEY=your-secret-key-here

# Optional: Vector Database Configuration
CHROMA_SERVER_HOST=0.0.0.0
CHROMA_SERVER_HTTP_PORT=8000

# Optional: Redis Configuration
REDIS_URL=redis://redis:6379

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Data Paths
DATA_PATH=/app/data
LOGS_PATH=/app/logs